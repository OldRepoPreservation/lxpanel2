    <property name="border_width">5</property>
    <property name="type_hint">dialog</property>
    <property name="program_name">CMake/GTK+ Test</property>
    <property name="version">0.1</property>
    <property name="copyright" translatable="yes">Copyright (C) 2011 by PCMan</property>
    <property name="comments" translatable="yes">Building GTK+ programs with CMake.	</property>
    <property name="website">http://pcmanx.blogspot.com/</property>
    <property name="license">Permission is hereby granted, free of charge, to any person obtaining a copy
